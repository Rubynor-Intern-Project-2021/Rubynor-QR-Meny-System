+import React, { useState } from 'react';
+import { ModalConfirm } from "./modal";
+import {storeModal, hideModal, storeAdmin, store} from "../configureStore";
+
+const AdminModalConfirm = () => {
+    const [modalStatus, setModalStatus] = useState(false);
+    const [item, setItem] = useState();
+
+    storeModal.subscribe(() => {
+        console.log(storeModal.getState())
+        setModalStatus(storeModal.getState())
+    });
+
+    storeAdmin.subscribe(() => {
+        console.log(storeAdmin.getState())
+        setItem(storeAdmin.getState())
+    });
+
+
+    let modal = null;
+    if(modalStatus) {
+        modal =
+            <div className="flex justify-center">
+                <ModalConfirm
+                    header="Slett meny"
+                    text="Ønsker du å slette denne menyen?"
+                    handleCancel={() => storeModal.dispatch(hideModal())}
+                    handleConfirm={() => {storeModal.dispatch(hideModal()); Routes.admin_set_item_status_path({item_id: item.id, status: 3}); }}
+                />
+            </div>
+    }
+
+    return (
+        <>
+            <div>
+                {modal}
+            </div>
+        </>
+    );
+};
+
+export default AdminModalConfirm;
