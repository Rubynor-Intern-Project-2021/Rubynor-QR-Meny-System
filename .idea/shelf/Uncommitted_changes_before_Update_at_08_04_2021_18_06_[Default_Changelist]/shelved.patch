<+><div class=\"flex lg:py-24  pt-20 justify-center\">\r\n\r\n  <!-- Kort -->\r\n  <div class=\"flex flex-col rounded-tl-3xl w-full bg-white lg:rounded-2xl lg:w-1/3\">\r\n    <div>\r\n      <p class=\"p-8 lg:mb-5 text-gray-900 font-semibold text-lg\">Handlekurv</p>\r\n    </div>\r\n\r\n    <div>\r\n      <% session[:cart].each do |item| %>\r\n        <%list = Hash.new%>\r\n        <% menu_item = MenuItem.find(item[\"item_id\"]) %>\r\n        <%list[menu_item.to_json] = item[\"amount\"] %>\r\n        <%#= link_to menu_item_path(item.menu_item) do %>\r\n        <!-- Hver rad-->\r\n        <div>\r\n          <%= react_component(\"cart/order_list\", { orderItems: list }) %>\r\n        </div>\r\n      <% end %>\r\n\r\n      <div class=\"px-8 \">\r\n        <div>\r\n          <%= button_to 'Slett handlekurv', empty_cart_path, method: :delete, data: { confirm: 'Are you sure?' }, class: \"text-white bg-gray-700 text-xs mt-3 p-2\" %>\r\n        </div>\r\n\r\n        <%= form_with local: true, url: make_order_path(restaurant_id: @restaurant.id), method: :post, builder: TailwindBuilder do |form| %>\r\n          <div class=\"mt-8\">\r\n            <%= form.label :\"Kommentar til kjøkkenet\" %>\r\n          </div>\r\n\r\n          <%= form.text_area :customer_info, placeholder: \"F.eks. uten løk\" %>\r\n          <div class=\"mt-4\">\r\n            <%= form.label :\"Bordnr\" %>\r\n          </div>\r\n          <%= form.text_field :location, placeholder: \"F.eks. 1234\", :maxlength => \"5\" %>\r\n\r\n          <div class=\" w-full bg-white mt-12\">\r\n            <div class=\"flex justify-between \">\r\n              <div class=\"font-semibold\">\r\n                Totalt\r\n              </div>\r\n              <div class=\"font-semibold\">\r\n                <%= react_component(\"total_price\") %>\r\n              </div>\r\n            </div>\r\n\r\n            <div class=\"actions \">\r\n\r\n              <%= form.submit 'Bestill', class: \"uppercase text-white bg-gray-700  mb-5 mt-1 py-2 w-full\" %>\r\n\r\n            </div>\r\n          </div>\r\n        <% end %>\r\n\r\n      </div>\r\n    </div>\r\n  </div>\r\n</div>\r\n\r\n\r\n
